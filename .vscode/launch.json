  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "echoo",
      "program": "${workspaceFolder}/bin/index.js",
      "args": ["gen"],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "console": "integratedTerminal",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "autoAttachChildProcesses": true,
      "skipFiles": [
        "<node_internals>/**"
      ],
    },
    {
      "type": "node",
      "request": "launch",
      "name": "js test",
      "program": "${workspaceFolder}/bin/index.js",
      "args": ["gen"],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "console": "integratedTerminal",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "autoAttachChildProcesses": true,
      "skipFiles": [
        "<node_internals>/**"
      ],
    }
  ]
}